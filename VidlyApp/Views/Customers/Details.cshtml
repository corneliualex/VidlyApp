@model VidlyApp.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<h2>Details</h2>

<h3>@Model.Name</h3>
<ul>
    <li>@Model.MembershipType.Name</li>
    @if (Model.BirthDate.HasValue)
    {
        <li>@Model.BirthDate.Value.ToString("dd-MM-yyy")</li>
    }
</ul>
